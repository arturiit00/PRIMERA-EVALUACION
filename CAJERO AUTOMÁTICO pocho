def banco():
    dinero=int(input("Ingresa una cantidad de dinero: "))
    seguir=True
    persona=int(input("Quieres sacar o meter dinero: "))
    if (persona=="S"):
        billetes=int(input("Cuanto dinero quieres sacar: "))
        resta=dinero - billetes
        print("resta")
    if (persona=="M"):
        billetes2=int(input("Cuanto dinero quieres meter: "))
        suma=dinero + billetes2
        
        
        
banco()
